module Rogue

class Template
  PROPERTIES
    name         : String
    placeholders : Token[]
    tokens       = Token[]

  METHODS
    method init( name, placeholders, tokens )

    method description->String
      return name

    method generate( type_args:Type[], type:Type )
      type.is_defined = true
      local class_def = Parser( tokens ).parse( Parser.ip_class )
      Organizer( type.module_context ).visit( type, class_def )
endClass
